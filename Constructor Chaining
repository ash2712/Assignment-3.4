public class MyClass{
	
	
	
		public static void main(String s[])
		{
		new Student(7,5);
		} 
}

class Student{
	
	Student()
    {
        System.out.println("The number you're looking for is -");
    }
 
    
    Student(int x, int y)
    {
        this();
        System.out.println(x * y);
    }
}
